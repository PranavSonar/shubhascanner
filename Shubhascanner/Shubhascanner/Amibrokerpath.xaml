        <Button Content="Browse" FontFamily="Times New Roman" FontSize="13" Height="25" HorizontalAlignment="Right" Margin="0,48,83,0" Name="button1" VerticalAlignment="Top" Width="72" Click="button1_Click" />
        <TextBox Height="23" HorizontalAlignment="Left" IsReadOnly="True" Margin="130,48,0,0" Name="Amiexepath" Text="C:\Program Files (x86)\AmiBroker" VerticalAlignment="Top" Width="186" />
        <Label Content="Amibroker exe path" FontFamily="Times New Roman" FontSize="12" Height="28" HorizontalAlignment="Left" Margin="10,48,0,0" Name="label1" VerticalAlignment="Top" />
        <Label Content="Select Amibroker Exe path " FontFamily="Times New Roman" FontSize="14" Height="28" HorizontalAlignment="Left" Margin="10,15,0,0" Name="label2" VerticalAlignment="Top" />
    </Grid>
</UserControl>
